citHeader("To cite package 'lookfor' in publications use:")
     
 year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
 vers <- paste("R package version", meta$Version)
 
 citEntry(entry="Manual",
		  title = "lookfor: Find What You're Looking For.",
		  author = personList(as.person("Thomas J. Leeper")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Thomas J. Leeper (",year,"). lookfor: Find What You're Looking For. ", vers, ".", sep=""))
